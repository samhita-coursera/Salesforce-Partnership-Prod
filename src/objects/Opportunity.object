    <validationRules>
        <fullName>Approval_Required_Reason_for_Loss</fullName>
        <active>false</active>
        <errorConditionFormula>ISPICKVAL( Approval_Stage__c , &quot;Closed - Lost&quot;) &amp;&amp;
 OR( No_response_dormant__c = FALSE,
     Rejected_internally__c = FALSE,
     No_interest_from_PoC_Instructor__c = FALSE,
     Cost_financial_reasons__c = FALSE,
     Other__c = FALSE)</errorConditionFormula>
        <errorMessage>Please provide a reason for why this proposal was lost/rejected.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>B2B_Progressing_to_Verbal_Commit_Stage</fullName>
        <active>false</active>
        <description>Deal terms and pricing must be approved before moving to or beyond verbal commit</description>
        <errorConditionFormula>and(  RecordType.Id =&quot;01236000000Lh7f&quot;,
or( ISPICKVAL( StageName , &quot;Verbal Commit&quot;) ,
ISPICKVAL( StageName , &quot;Closed Won&quot;) ,
Probability =100),
 OR( Deal_Terms_Approved__c =FALSE,
 Discount_Approved__c =FALSE))</errorConditionFormula>
        <errorDisplayField>Deal_Terms_Approved__c</errorDisplayField>
        <errorMessage>Please submit the opportunity for approval (using the &apos;Submit for Approval&apos; button) before moving the stage to Verbal Commitment. Thanks :)</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>B2B_Required_Loss_Reasons</fullName>
        <active>false</active>
        <description>At least one reason must be selected if moving stage to &quot;Lost&quot;</description>
        <errorConditionFormula>and(RecordType.Id=&quot;01236000000Lh7f&quot;,
and(ISPICKVAL(StageName,&quot;Lost/Dormant&quot;),
AND(Weak_no_interest__c=FALSE,  
Cost_pricing__c =FALSE, 
Other__c=FALSE, 
Rejected_internally__c=FALSE, 
No_response_dormant__c=FALSE)))</errorConditionFormula>
        <errorDisplayField>Rejected_internally__c</errorDisplayField>
        <errorMessage>If the opp was lost, please check at least one reason this section so we can understand where/why we&apos;re losing. If you&apos;ve checked &quot;Other,&quot; please add some notes in the &quot;Notes/details on loss&quot; box.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Capstone_Transition_Plan_Required</fullName>
        <active>false</active>
        <description>If Specialization currently uses the old Capstone model (aka Locked Capstone), TAM must provide details around how we&apos;ll transition to the new model</description>
        <errorConditionFormula>ISPICKVAL( Capstone_Model__c , &quot;Locked Capstone&quot;)  &amp;&amp;  ISBLANK( Capstone_Transition_Plan__c )</errorConditionFormula>
        <errorDisplayField>Capstone_Transition_Plan__c</errorDisplayField>
        <errorMessage>Please note that if the Capstone model is &quot;Locked&quot;, a transition plan is required.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Cert_Final_Approval</fullName>
        <active>false</active>
        <errorConditionFormula>ISPICKVAL(  StageName  , &quot;Closed / Won&quot;) &amp;&amp;  Content_Type__c = &quot;Certificate&quot; &amp;&amp; Signoff_from_Nikhil__c = FALSE</errorConditionFormula>
        <errorMessage>This Certificate Approval does not yet have final signoff from Nikhil as the approver. Please resolve this before final approval, and moving this Certificate into Production.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Cert_Submission_EoI</fullName>
        <active>false</active>
        <errorConditionFormula>Content_Type__c == &quot;Certificate&quot;  &amp;&amp; 
 ISPICKVAL( StageName , &quot;Commitment&quot;) &amp;&amp;
  ISBLANK( Cert_EoI__c )</errorConditionFormula>
        <errorMessage>Please upload the EoI for this Certificate submission.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Commitment_Checklist_Rule</fullName>
        <active>false</active>
        <errorConditionFormula>AND 
(RecordType.Name = &quot;University Core Opportunity&quot;, 
ISCHANGED(StageName), 
ISPICKVAL(PRIORVALUE(StageName), &quot;Commitment&quot;), 
NOT(ISPICKVAL(StageName, &quot;Closed Lost&quot;)), 
NOT(ISPICKVAL(StageName, &quot;Target Identified&quot;)), 
NOT(ISPICKVAL(StageName, &quot;Qualification&quot;)), 
NOT(ISPICKVAL(StageName, &quot;Interest Development&quot;)), 
OR(NOT(DM_2_committed_for_content_creation__c), 
NOT(DM_1_committed_for_content_creation__c)) 
)</errorConditionFormula>
        <errorMessage>Please make sure that Commitment checklist is completed before moving to next stage. If any of the criteria is not fulfilled, please Close Lost the opportunity.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>E_staff_approval_decision_b4_VM_approval</fullName>
        <active>true</active>
        <errorConditionFormula>AND(
NOT(Is_approved_by_E_Staff__c),
Is_RFP_Approved_by_VM__c  
)</errorConditionFormula>
        <errorMessage>Please enter if RFP is approved by E-staff. 
If approved, mark &quot;Is RFP Approved by E-Staff&quot; true. 
If rejected, select reason for rejection and mark opportunity as closed lost.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Get_Approval_To_Change_SubmissionStage</fullName>
        <active>true</active>
        <description>Rule to ensure that the UAB approval is received before moving the new Industry Partner prospect from Proposal Submission stage to any other stage</description>
        <errorConditionFormula>AND(
RecordType.Name = &quot;New Industry Partner Opportunity&quot; , NOT(ISPICKVAL(StageName, &quot;Proposal Submission&quot;)), ISPICKVAL(PRIORVALUE(StageName), &quot;Proposal Submission&quot;),
NOT(ISPICKVAL(StageName, &quot;Closed Lost&quot;)), 
NOT(ISPICKVAL(StageName, &quot;Closed / Lost or Dormant&quot;)),
ISCHANGED(StageName), 
UAB_Approved__c = False)</errorConditionFormula>
        <errorMessage>Please get UAB approval before changing the opportunity stage</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Get_Approval_To_Qualify_the_Opportunity</fullName>
        <active>true</active>
        <description>Rule to ensure that the internal approval is received before moving the new Industry Partner prospect from Qualifying stage to any other stage</description>
        <errorConditionFormula>AND(
RecordType.Name = &quot;New Industry Partner Opportunity&quot; , NOT(ISPICKVAL(StageName, &quot;Qualifying&quot;)), 
NOT(ISPICKVAL(StageName, &quot;Closed Lost&quot;)), 
NOT(ISPICKVAL(StageName, &quot;Closed / Lost or Dormant&quot;)),
ISCHANGED(StageName),
Internally_Approved_by_CBO__c = False)</errorConditionFormula>
        <errorMessage>Please get internal approval from Nikhil before changing the opportunity stage</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Inbound_Proposal</fullName>
        <active>false</active>
        <errorConditionFormula>ISPICKVAL(  Lead_Type__c  , &quot;Inbound&quot;)  &amp;&amp;  ISPICKVAL( StageName, &quot;Syllabus Submitted&quot;)  &amp;&amp;  ISBLANK(  Inbound_Proposal__c  )</errorConditionFormula>
        <errorDisplayField>Inbound_Proposal__c</errorDisplayField>
        <errorMessage>All inbound requests must have a corresponding proposal</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Industry_Partner_Stage_Transition_Rule</fullName>
        <active>true</active>
        <errorConditionFormula>RecordType.Name = &quot;New Industry Partner Opportunity&quot; &amp;&amp; ((CASE(StageName, 
&quot;Qualification&quot;,1, 
&quot;Proposal Development&quot;,2, 
&quot;Proposal Submission&quot;,3, 
&quot;Contract Creation&quot;,4,  
&quot;Closed / Won&quot;,5 ,0) 
&gt;
(CASE(PRIORVALUE(StageName) , 
&quot;Qualification&quot;,1, 
&quot;Proposal Development&quot;,2, 
&quot;Proposal Submission&quot;,3, 
&quot;Contract Creation&quot;,4,  
&quot;Closed / Won&quot;,5 ,0)+1)
) ||
(CASE( StageName, 
&quot;Qualification&quot;,1, 
&quot;Proposal Development&quot;,2, 
&quot;Proposal Submission&quot;,3, 
&quot;Contract Creation&quot;,4,  
&quot;Closed / Won&quot;,5 ,0) 
&lt;
(CASE(PRIORVALUE(StageName) , 
&quot;Qualification&quot;,1, 
&quot;Proposal Development&quot;,2, 
&quot;Proposal Submission&quot;,3, 
&quot;Contract Creation&quot;,4,  
&quot;Closed / Won&quot;,5 ,0))))</errorConditionFormula>
        <errorMessage>Please select only next immediate stage available.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Interest_Development_Checklist_Rule</fullName>
        <active>false</active>
        <errorConditionFormula>AND 
(RecordType.Name = &quot;University Core Opportunity&quot;, 
ISCHANGED(StageName), 
ISPICKVAL(PRIORVALUE(StageName), &quot;Interest Development&quot;), 
NOT(ISPICKVAL(StageName, &quot;Closed Lost&quot;)), 
NOT(ISPICKVAL(StageName, &quot;Target Identified&quot;)), 
NOT(ISPICKVAL(StageName, &quot;Qualification&quot;)), 
OR(NOT( DM_2_received_campaign_docs__c ), 
NOT( DM_2_understood_campaign__c ), 
NOT(DM_2_understood_opportunity__c), 
NOT( Resources_identified_by_Instructor__c ), 
NOT(Sent_RFP_doc_to_Partner__c)) 
)</errorConditionFormula>
        <errorMessage>Please make sure that Interest Development checklist is completed. If any of the required criteria is not fulfilled then please Close Lost the opportunity.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>New_IP_Required_Details_Before_Approved</fullName>
        <active>true</active>
        <description>Fill in the Branding notes, Pedagogy and Summary fields before marking the opportunity as Approved</description>
        <errorConditionFormula>AND( 
OR(Internally_Approved_by_CBO__c , UAB_Approved__c ), 
OR(ISBLANK(Branding_Notes__c), ISBLANK(Pedagogy__c) , ISBLANK(Content_Summary__c)) 
)</errorConditionFormula>
        <errorDisplayField>Content_Summary__c</errorDisplayField>
        <errorMessage>Please fill in Content Summary, Pedagogy and Branding notes before changing the approval status</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>New_IP_Required_Loss_Reasons</fullName>
        <active>true</active>
        <description>At least one reason for loss must be selected if the stage is moved to Closed Lost</description>
        <errorConditionFormula>AND( 
RecordType.Name= &quot;New Industry Partner Opportunity&quot;, 
OR(ISPICKVAL(StageName, &quot;Closed Lost&quot;), ISPICKVAL(StageName, &quot;Closed / Lost or Dormant&quot;)), 
AND(Cost_financial_reasons__c=FALSE, 
No_response_dormant__c=FALSE, 
Other__c=FALSE, 
Rejected_internally__c=FALSE, 
Rejected_by_UAB__c=FALSE, No_interest_from_PoC_Instructor__c=FALSE) 
)</errorConditionFormula>
        <errorDisplayField>Rejected_internally__c</errorDisplayField>
        <errorMessage>If the new partner prospect is lost, please check at least one reason for loss/dropped so we can understand where/why we&apos;re losing. If you&apos;ve checked &quot;Other,&quot; please add some notes in the &quot;Notes/details on loss&quot; box.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>No_New_Opty_4_Rejected_IP_Account</fullName>
        <active>true</active>
        <description>New opportunity cannot be created if the account is a rejected Industry partner account</description>
        <errorConditionFormula>AND(Account.Is_Account_Rejected__c, 
OR (Account.RecordTypeId = &quot;01236000001DiGZAA0&quot;, Account.RecordTypeId = &quot;01236000001DiGZ&quot;), 
NOT(ISPICKVAL(StageName, &quot;Closed Lost&quot;)), 
NOT(ISPICKVAL(StageName, &quot;Closed / Lost or Dormant&quot;)) 
)</errorConditionFormula>
        <errorMessage>Cannot create or edit opportunity related to rejected industry partner account. Please contact your system administrator for help</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Partnerships_Required_Loss_Reasons</fullName>
        <active>true</active>
        <description>At least one reason for loss must be selected if the Industry partner or University partner opportunity stage is moved to Closed-Lost</description>
        <errorConditionFormula>AND( 
OR(RecordType.Name =&quot;University Core Opportunity&quot;,
RecordType.Name =&quot;Industry Partner Content Opportunity&quot;),
NOT(Account.Is_Account_Rejected__c), 
OR(ISPICKVAL(StageName, &quot;Closed Lost&quot;), ISPICKVAL(StageName, &quot;Closed / Lost or Dormant&quot;)),
AND(Cost_financial_reasons__c=FALSE, No_response_dormant__c=FALSE, 
Other__c=FALSE, 
